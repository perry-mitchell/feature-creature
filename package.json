  "dependencies": {
    "chalk": "^5.0.1",
    "marked": "^4.0.16",
    "minimist": "^1.2.6"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.41",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3"
  }
